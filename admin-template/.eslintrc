{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "prettier",
    "prettier/vue",
    "plugin:prettier/recommended",
    "plugin:vue/essential",
    "@vue/standard"
  ],
  "plugins": ["prettier"],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "indent": "off",
    "no-return-await": "off",
    "no-prototype-builtins": "off",
    "space-before-function-paren": "off"
  }
}
