{
  "extends": ["stylelint-config-recommended", "stylelint-config-rational-order"],
  "plugins": ["stylelint-scss", "stylelint-order"],
  "rules": {
      "order/order": ["custom-properties",
      "dollar-variables",
      "declarations",
      "rules",
      "at-rules"
    ],
    "no-descending-specificity": null,
    "block-no-empty": null,
  }
}
